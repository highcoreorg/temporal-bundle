{
    "name": "highcore/temporal-bundle",
    "description": "Command Runner and Activity Registry for Temporal",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "loper",
            "email": "bizrenay@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "temporal/sdk": "^2.10",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/console": "^6.4 || ^7.0",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/property-info": "^6.4 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "autoload": {
        "psr-4": {
            "Highcore\\TemporalBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Highcore\\TemporalBundle\\": "tests"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
